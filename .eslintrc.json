{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "no-tabs": "off",
    "consistent-return": "off",
    "linebreak-style": "off",
    "no-underscore-dangle": "off",
    "react/prop-types": 0,
    "react/jsx-props-no-spreading": "off",
    "arrow-body-style":"off"
  }
}
